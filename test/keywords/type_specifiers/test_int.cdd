/**
 * @file test_int.cdd
 * @brief int 关键字测试用例
 * 
 * 测试 int 类型的各种用法
 */

// 1. 基本 int 变量
int basic_int;

// 2. int 初始化
int init_int = 42;

// 3. signed int
signed int signed_i = -2147483648;

// 4. signed（等价于 signed int）
signed just_signed = -1000;

// 5. unsigned int
unsigned int unsigned_i = 4294967295;

// 6. unsigned（等价于 unsigned int）
unsigned just_unsigned = 1000;

// 7. int 数组
int int_array[100];

// 8. int 数组初始化
int init_array[] = {10, 20, 30, 40, 50};

// 9. int 指针
int* int_ptr;

// 10. const int
const int const_int = 100;

// 11. volatile int
volatile int volatile_int;

// 12. int 指针的指针
int** ptr_to_int_ptr;

// 13. int 数组的指针
int (*ptr_to_int_array)[10];

// 14. 指针数组
int* ptr_array[5];

// 15. 返回 int 的函数
int get_int(void) {
    return 123;
}

// 16. int 参数
void accept_int(int n) {
    return;
}

// 17. 函数指针，返回 int
int (*int_func_ptr)(void);

// 18. 函数指针，接受 int 参数
void (*func_with_int)(int);

// 19. 多维 int 数组
int multi_int[3][4][5];

// 20. int 类型转换
char int_to_char(int n) {
    return (char)n;
}

// 21. int 算术运算
int int_arithmetic() {
    int a = 100;
    int b = 30;
    int sum = a + b;
    int diff = a - b;
    int prod = a * b;
    int quot = a / b;
    int rem = a % b;
    return sum;
}

// 22. int 位运算
int int_bitwise() {
    int a = 0x0F0F0F0F;
    int b = 0xF0F0F0F0;
    int and_result = a & b;
    int or_result = a | b;
    int xor_result = a ^ b;
    int not_result = ~a;
    int left_shift = a << 4;
    int right_shift = b >> 4;
    return or_result;
}

// 23. int 比较运算
int int_compare() {
    int a = 10;
    int b = 20;
    int eq = (a == b);
    int ne = (a != b);
    int lt = (a < b);
    int le = (a <= b);
    int gt = (a > b);
    int ge = (a >= b);
    return eq;
}

// 24. int 逻辑运算
int int_logical() {
    int a = 1;
    int b = 0;
    int and_result = a && b;
    int or_result = a || b;
    int not_result = !a;
    return and_result;
}

// 25. int 自增自减
int int_incr_decr() {
    int a = 10;
    int pre_incr = ++a;
    int post_incr = a++;
    int pre_decr = --a;
    int post_decr = a--;
    return a;
}

// 26. int 复合赋值
int int_compound_assign() {
    int a = 100;
    a += 10;
    a -= 5;
    a *= 2;
    a /= 3;
    a %= 7;
    a &= 0xFF;
    a |= 0x100;
    a ^= 0x55;
    a <<= 2;
    a >>= 1;
    return a;
}

// 27. int 三元运算
int int_ternary(int cond) {
    return cond ? 1 : 0;
}

// 28. int 逗号表达式
int int_comma() {
    int a;
    int b;
    a = (b = 5, b + 10);
    return a;
}

int main(void) {
    basic_int = 999;
    int_ptr = &basic_int;
    
    int local_int = 100;
    unsigned local_uint = 200;
    
    return 0;
}
